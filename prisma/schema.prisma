generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model edges {
  id    Int  @id @default(autoincrement())
  Graph Json
}

model nodes {
  id    Int  @id @default(autoincrement())
  Graph Json
}

model graphs {
  id        Int      @id @default(autoincrement())
  UserID    String   @db.VarChar(256)
  name      String   @db.VarChar(256)
  createdAt DateTime
  updatedAt DateTime
  createdBy String   @db.VarChar(256)
  updatedBy String   @db.VarChar(256)
  deleted   Boolean
}

model dimensions {
  id    Int  @id @default(autoincrement())
  Graph Json
}
